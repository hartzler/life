#update.tab-pane.fade
  .form.well
    .controls
      %label To
      %select{"data-bind"=>"options: profiles, optionsText: 'display', value: updateTo, optionsCaption: 'Choose...'"}
    .controls
      %label Status Update
      %textarea.input-xlarge{"data-bind"=>"value: updateContent"}
    %button{:type=>"submit", "data-bind"=>"click: update"} Submit
      
